{
  "name": "@adnbn/inject-script",
  "version": "0.2.2",
  "description": "A lightweight, TypeScript-ready library for injecting JavaScript functions or external scripts into Chrome extension tabs and frames (Manifest V2 & V3).",
  "keywords": [
    "browser",
    "addon",
    "extension",
    "google",
    "chrome",
    "opera",
    "edge",
    "firefox",
    "safari",
    "api",
    "script"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:addonbone/inject-script.git"
  },
  "license": "MIT",
  "author": "Addon Bone",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write .",
    "build": "tsup",
    "build:watch": "tsup --watch"
  },
  "dependencies": {
    "@adnbn/browser": "^0.1.7",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
